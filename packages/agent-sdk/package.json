{
  "name": "@deepractice-ai/agent-sdk",
  "version": "0.6.4",
  "description": "High-performance Agent SDK with RxJS event-driven architecture",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/api/common/index.d.ts",
      "import": "./dist/api/common/index.js"
    },
    "./common": {
      "types": "./dist/api/common/index.d.ts",
      "import": "./dist/api/common/index.js"
    },
    "./server": {
      "types": "./dist/api/server/index.d.ts",
      "import": "./dist/api/server/index.js"
    },
    "./browser": {
      "types": "./dist/api/browser/index.d.ts",
      "import": "./dist/api/browser/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run"
  },
  "keywords": [
    "ai",
    "agent",
    "sdk",
    "claude",
    "rxjs",
    "deepractice"
  ],
  "author": "Deepractice Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/Agent.git"
  },
  "bugs": {
    "url": "https://github.com/Deepractice/Agent/issues"
  },
  "homepage": "https://github.com/Deepractice/Agent#readme",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.30",
    "@anthropic-ai/sdk": "^0.32.1",
    "@deepracticex/logger": "^1.1.1",
    "better-sqlite3": "^12.4.1",
    "drizzle-orm": "^0.44.7",
    "eventemitter3": "^5.0.1",
    "neverthrow": "^8.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tsyringe": "^4.10.0",
    "xstate": "^5.24.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@deepracticex/vitest-cucumber": "^1.4.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.9.2",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
