    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.14.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
