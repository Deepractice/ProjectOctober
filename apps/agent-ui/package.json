{
  "name": "@project-october/agent-ui",
  "version": "1.10.4",
  "description": "A web-based UI for AI Agent",
  "private": true,
  "type": "module",
  "main": "server/index.js",
  "bin": {
    "agent-ui": "server/index.js"
  },
  "files": [
    "server/",
    "dist/",
    "README.md"
  ],
  "homepage": "https://agent-ui.deepractice.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepractice/ProjectOctober.git"
  },
  "bugs": {
    "url": "https://github.com/deepractice/ProjectOctober/issues"
  },
  "scripts": {
    "dev": "concurrently --kill-others --raw --prefix none \"npm run server\" \"npm run client\"",
    "server": "NODE_ENV=production node server/index.js",
    "client": "vite --host --logLevel error",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run build && npm run server",
    "release": "./release.sh",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "keywords": [
    "claude coode",
    "ai",
    "anthropic",
    "ui",
    "mobile"
  ],
  "author": "Claude Code UI Contributors",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.29",
    "@codemirror/commands": "^6.10.0",
